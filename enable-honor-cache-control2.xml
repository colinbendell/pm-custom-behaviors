<cache:honor-cc2>
	<status>on</status>
	<use-directives>
		<max-age>on</max-age>
		<no-cache>off</no-cache>
		<no-store>off</no-store>
		<no-transform>off</no-transform>
		<post-check>off</post-check>
		<pre-check>off</pre-check>
		<private>off</private>
		<proxy-revalidate>off</proxy-revalidate>
		<public>off</public>
		<s-maxage>on</s-maxage>
	</use-directives>
</cache:honor-cc2>
<match:variable name="PMUSER_HONORCC2" value="*max-age*" value-wildcard="on">
	<cache:honor-cc2>
		<status>on</status>
		<use-directives.max-age>on</use-directives.max-age>
	</cache:honor-cc2>
</match:variable>
<match:variable name="PMUSER_HONORCC2" value="*must-revalidate*" value-wildcard="on">
	<cache:honor-cc2>
		<status>on</status>
		<use-directives.must-revalidate>on</use-directives.must-revalidate>
	</cache:honor-cc2>
</match:variable>
