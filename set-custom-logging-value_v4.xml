<assign:variable>
    <name>PMUSER_LOG_PROTO_V</name>
    <value>h1</value>
    <hidden>on</hidden>
</assign:variable>
    <assign:variable>
    <name>PMUSER_LOG_SNI_V</name>
    <value></value>
    <hidden>on</hidden>
</assign:variable>

<match:request.protocol value="HTTPS">
    <assign:variable>
        <name>PMUSER_LOG_SNI_V</name>
        <value>%(AK_TLS_CIPHER_NAME)</value>
        <hidden>on</hidden>
    </assign:variable>
    <match:variable name="AK_TLS_SNI_NAME" value="EMPTY_STRING" result="false">
        <assign:variable>
            <name>PMUSER_LOG_SNI_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>

    <assign:variable>
        <name>PMUSER_LOG_PROTO_V</name>
        <value>tls</value>
        <hidden>on</hidden>
    </assign:variable>

    <match:variable name="AK_PROTOCOL_NEGOTIATION" value="http2">
        <assign:variable>
            <name>PMUSER_LOG_PROTO_V</name>
            <value>h2</value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
</match:request.protocol>

<assign:variable>
<name>PMUSER_LOG_CLIENT_RTT_V</name>
<value>%(AK_CLIENT_RTT)</value>
<hidden>on</hidden>
</assign:variable>

<assign:variable>
    <name>PMUSER_LOG_ARLID_V</name>
    <value>%(AK_ARLID).%(AK_ARLVERSION)</value>
    <hidden>on</hidden>
</assign:variable>

<assign:variable>
    <name>PMUSER_LOG_CLIENT_PORT_V</name>
    <value>%(AK_TRUE_CLIENT_PORT)</value>
    <hidden>on</hidden>
</assign:variable>

<assign:variable>
    <name>PMUSER_LOG_AK_REQUESTID_V</name>
    <value>%(AK_REQUEST_ID)</value>
    <hidden>on</hidden>
</assign:variable>

<assign:variable>
    <name>PMUSER_LOG_AK_GHOSTIP_V</name>
    <value>%(AK_GHOST_IP)</value>
    <hidden>on</hidden>
</assign:variable>

<match:metadata-stage value="client-response">
    <assign:variable>
        <name>PMUSER_LOG_PREFIX_V</name>
        <value>%(PMUSER_CUSTOM_LOG_VALUE)</value>
        <hidden>on</hidden>
    </assign:variable>

    <assign:variable>
        <name>PMUSER_LOG_CACHE_STATE_V</name>
        <value>%(AK_CONTENT_SOURCE)</value>
        <hidden>on</hidden>
    </assign:variable>
    <assign:variable>
        <name>PMUSER_LOG_REMOTE_CACHE_STATE_V</name>
        <value>%(PMUSER_REMOTE_CACHE_STATE)</value>
        <hidden>on</hidden>
    </assign:variable>
    <assign:variable>
        <name>PMUSER_LOG_TTFB_V</name>
        <value>%(AK_CLIENT_TURNAROUND_TIME)</value>
        <hidden>on</hidden>
    </assign:variable>

    <match:response.header name="X-Request-ID">
        <assign:extract-value>
            <variable-name>TMP_LOG_REQUESTID</variable-name>
            <location>Response_Header</location>
            <location-id>X-Request-ID</location-id>
        </assign:extract-value>
        <assign:variable>
            <name>PMUSER_LOG_ORIGIN_REQUESTID_V</name>
            <value>%(TMP_LOG_REQUESTID)</value>
            <hidden>on</hidden>
        </assign:variable>
    </match:response.header>

    <match:variable name="PMUSER_LOG_PREFIX" value="0 off false">
        <assign:variable>
            <name>PMUSER_LOG_PREFIX_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
    <match:variable name="PMUSER_LOG_CLIENT_RTT" value="0 off false">
        <assign:variable>
            <name>PMUSER_LOG_CLIENT_RTT_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
    <match:variable name="PMUSER_LOG_CLIENT_RTT" value="0 off false">
        <assign:variable>
            <name>PMUSER_LOG_CLIENT_RTT_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
    <match:variable name="PMUSER_LOG_PROTO" value="0 off false">
        <assign:variable>
            <name>PMUSER_LOG_CLIENT_RTT_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
    <match:variable name="PMUSER_LOG_SNI" value="0 off false">
        <assign:variable>
            <name>PMUSER_LOG_CLIENT_RTT_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
    <match:variable name="PMUSER_LOG_CLIENT_PORT" value="0 off false">
        <assign:variable>
            <name>PMUSER_LOG_CLIENT_RTT_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
    <match:variable name="PMUSER_LOG_CACHE_STATE" value="0 off false">
        <assign:variable>
            <name>PMUSER_LOG_CLIENT_RTT_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
    <match:variable name="PMUSER_LOG_REMOTE_CACHE_STATE" value="0 off false">
        <assign:variable>
            <name>PMUSER_LOG_CLIENT_RTT_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
    <match:variable name="PMUSER_LOG_ORIGIN_REQUESTID" value="0 off false">
        <assign:variable>
            <name>PMUSER_LOG_CLIENT_RTT_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
    <match:variable name="PMUSER_LOG_AK_REQUESTID" value="0 off false">
        <assign:variable>
            <name>PMUSER_LOG_CLIENT_RTT_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
    <match:variable name="PMUSER_LOG_ARLID" value="0 off false">
        <assign:variable>
            <name>PMUSER_LOG_ARLID_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
    <match:variable name="PMUSER_LOG_AK_GHOSTIP" value="1 on true" result="false">
        <assign:variable>
            <name>PMUSER_LOG_CLIENT_RTT_V</name>
            <value></value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>

    <!-- Custom Logging is limitted to 40 characters -->
    <!-- Worst case: RTT: 4, SNI: 4; PROTO: 2; PORT: 5; CACHE: 1; CACHE-REMOTE: 1; TTFB: 4; X-Req: 8; X-Akam-Req: 16; Ghost IP: 15 -->
    <!-- Likely case: RTT: 3, SNI: 0; PROTO: 2; PORT: 5; CACHE: 1; CACHE-REMOTE: 1; TTFB: 3; X-Req: 8; X-Akam-Req: 16; Ghost IP: 15 -->
    <assign:variable>
        <name>PMUSER_CUSTOM_LOG_VALUE</name>
        <value>%(PMUSER_LOG_PREFIX_V)%(PMUSER_LOG_CLIENT_RTT_V)%(PMUSER_LOG_PROTO_V)%(PMUSER_LOG_SNI_V)%(PMUSER_LOG_CLIENT_PORT_V)%(PMUSER_LOG_CACHE_STATE_V)%(PMUSER_LOG_REMOTE_CACHE_STATE_V)%(PMUSER_LOG_TTFB_V)%(PMUSER_LOG_ORIGIN_REQUESTID_V).%(PMUSER_LOG_AK_REQUESTID_V)%(PMUSER_LOG_AK_GHOSTIP_V)%(PMUSER_LOG_ARLID_V)</value>
        <hidden>on</hidden>
    </assign:variable>
    <match:variable name="PMUSER_LOG_IS_DELIMITED" value="1 on true">
        <assign:variable>
            <name>PMUSER_CUSTOM_LOG_VALUE</name>
            <value>%(PMUSER_LOG_PREFIX_V)%(PMUSER_LOG_CLIENT_RTT_V);%(PMUSER_LOG_PROTO_V);%(PMUSER_LOG_SNI_V);%(PMUSER_LOG_CLIENT_PORT_V);%(PMUSER_LOG_CACHE_STATE_V);%(PMUSER_LOG_REMOTE_CACHE_STATE_V);%(PMUSER_LOG_TTFB_V);%(PMUSER_LOG_ORIGIN_REQUESTID_V);%(PMUSER_LOG_AK_REQUESTID_V);%(PMUSER_LOG_AK_GHOSTIP_V)%(PMUSER_LOG_ARLID_V)</value>
            <hidden>on</hidden>
        </assign:variable>
    </match:variable>
</match:metadata-stage>